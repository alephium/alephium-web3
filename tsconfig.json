{
  "compilerOptions": {
    "target": "es2020",
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "declaration": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "noImplicitOverride": true
  }
}
